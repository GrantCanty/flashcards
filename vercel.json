{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "client/build" }
      },
      {
        "src": "server/main.go",
        "use": "@vercel/go"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/main.go"
      },
      {
        "src": "/(.*)",
        "dest": "/client/build/index.html"
      }
    ]
  }
  